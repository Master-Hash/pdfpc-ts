{
  "name": "pdfpc-ts",
  "version": "0.1.0",
  "description": "Fastly Compute static site",
  "author": "you@example.com",
  "type": "module",
  "devDependencies": {
    "@fastly/compute-js-static-publish": "^7.0.3"
  },
  "dependencies": {
    "@fastly/js-compute": "^3.37.0"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "private": true,
  "scripts": {
    "dev:publish": "compute-js-static-publish publish-content --local",
    "dev:start": "fastly compute serve",
    "fastly:deploy": "fastly compute publish",
    "fastly:publish": "compute-js-static-publish publish-content",
    "build": "js-compute-runtime ./src/index.js ./bin/main.wasm --enable-aot"
  }
}
